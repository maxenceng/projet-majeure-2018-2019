{
  "extends": ["airbnb"],
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "linebreak-style": 0,
    "react/jsx-one-expression-per-line": 0,
    "react/sort-comp": 0,
    "react/button-has-type": 0
  },
  "globals": {
    "localStorage": true,
    "window": true
  }
}